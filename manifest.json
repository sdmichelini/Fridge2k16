{
  "manifest_version": 2,

  "name": "PokeNope",
  "description": "Removes Pokemon Go related stuff from your Facebook News Feed",
  "version": "1.0",
  "content_scripts": [
  {
    "matches": ["*://*.facebook.com/*"],
    "js": ["pokenope.js"],
    "run_at": "document_end"
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "pokenope.html",
    "default_title": "PokeNope!"
  }
}